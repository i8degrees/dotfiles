<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>UserName</key>
    <string>jeff</string>
    <key>Label</key>
    <string>org.local.mpdscribble</string>
    <key>ProgramArguments</key>
    <array>
      <string>sh</string>
      <string>-i</string>
      <string>-c</string>
      <string>$SHELL --login -c "mpdscribble --no-daemon --conf ~/.config/mpd/mpdscribble.conf"</string>
    </array>

    <key>Disabled</key>
    <false/>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>NetworkState</key>
    <true/>
  </dict>
</plist>
