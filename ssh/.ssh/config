# ~/.ssh/config:jeff
#
# Default user ssh_config (5) setup
#
# NOTE(JEFF): This is a user template for
# use as a guide or reference for appending 
# additional hosts to this file.
#

Host *
  AddKeysToAgent yes
  Compression no
  IdentityFile ~/.ssh/id_ed25519
  #IdentityFile ~/.ssh/id_rsa
  #IdentityFile ~/.ssh/id_ecdsa

Host github.com
  User git
  IdentityFile ~/.ssh/id_rsa-github-nopassphrase
  IdentityFile ~/.ssh/id_ed25519-github

Host git.fs1.home
  User git
  IdentityFile ~/.ssh/id_rsa-github-nopassphrase
  IdentityFile ~/.ssh/id_ed25519-github

