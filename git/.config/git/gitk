#
#   git/gitk
#
# Global (user-scoped) git repository browser
#
# See also,
#   git(1), gitk(1)

set mainfont {{Helvetica Rounded LT Std} 13}
set textfont {{Envy Code R} 12}
set uifont {{Helvetica Rounded LT Std} 12 bold}
set tabstop 8
set findmergefiles 0
set maxgraphpct 50
set maxwidth 16
set cmitmode patch
set wrapcomment none
set autoselect 1
set autosellen 40
set showneartags 1
set hideremotes 0
set showlocalchanges 1
set datetimeformat {%Y-%m-%d %H:%M:%S}
set limitdiffs 1
set uicolor #d9d9d9
set want_ttk 1
set bgcolor #000000
set fgcolor #ee8900
set colors {green red blue magenta darkgrey brown orange}
set diffcolors {{#ff0096} #00a0d4 #00b600}
set markbgcolor #ee8bbb
set diffcontext 3
set selectbgcolor #eee9a1
set extdifftool meld
set perfile_attrs 0
set geometry(main) 989x838+5+27
set geometry(state) normal
set geometry(topwidth) 989
set geometry(topheight) 377
set geometry(pwsash0) "416 1"
set geometry(pwsash1) "688 1"
set geometry(botwidth) 974
set geometry(botheight) 456
set permviews {}
